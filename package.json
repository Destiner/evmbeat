{
  "name": "evmbeat",
  "version": "1.0.0",
  "description": "EVMBeat",
  "author": "Timur Badretdinov <destinerx@gmail.com>",
  "keywords": [
    "vue",
    "ethereum",
    "evm"
  ],
  "license": "UNLICENSED",
  "repository": {
    "type": "git",
    "url": "git://github.com/Destiner/evmbeat.git"
  },
  "type": "module",
  "scripts": {
    "build": "vite build",
    "dev": "vite",
    "lint": "npm run lint:prettier && npm run lint:eslint && npm run lint:stylelint",
    "lint:prettier": "prettier -c \"src/**/*.{json,js,ts,vue}\"",
    "lint:eslint": "eslint \"src/**/*.{js,ts,vue}\"",
    "lint:stylelint": "stylelint \"src/**/*.{css,vue}\"",
    "typecheck": "npx tsc --noEmit && npx vue-tsc --noEmit"
  },
  "dependencies": {
    "vue": "^3.3.13",
    "vue-router": "^4.2.5"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^6.16.0",
    "@vitejs/plugin-vue": "^5.0.0",
    "@vue/compiler-sfc": "^3.3.13",
    "eslint": "^8.56.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-vue": "^9.19.2",
    "prettier": "^3.1.1",
    "stylelint": "^16.1.0",
    "stylelint-config-property-sort-order-smacss": "^10.0.0",
    "stylelint-config-recommended-vue": "^1.5.0",
    "stylelint-config-standard": "^36.0.0",
    "typescript": "^5.3.3",
    "vite": "^5.0.10",
    "vue-tsc": "^1.8.27"
  }
